<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <META_PATH>$(MSBuildThisFileDirectory)</META_PATH>
    <VCP_PATH>$(MSBuildThisFileDirectory)\externals</VCP_PATH>
    <DESERT_PATH>$(MSBuildThisFileDirectory)\externals\desert</DESERT_PATH>
    <UdmCliGen>$(MSBuildThisFileDirectory)\externals\common-scripts\UdmCliGen\UdmCliGen.exe</UdmCliGen>
    <THIRDPARTY_PATH>$(MSBuildThisFileDirectory)\3rdParty</THIRDPARTY_PATH>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>..\..\..\..\..\Build\$(Platform)\VC10\$(Configuration)\</OutDir>
    <IntDir>$(OutDir)obj\src\$(ProjectName)\</IntDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(META_PATH)\generated;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>$(META_PATH)\src\bin;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="META_PATH">
      <Value>$(META_PATH)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="VCP_PATH">
      <Value>$(VCP_PATH)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="DESERT_PATH">
      <Value>$(DESERT_PATH)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="UdmCliGen">
      <Value>$(UdmCliGen)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="THIRDPARTY_PATH">
      <Value>$(THIRDPARTY_PATH)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>